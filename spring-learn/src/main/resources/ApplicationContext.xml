<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="myTestBean" class="com.ustc.learn.beans.MyTestBean" scope="prototype" init-method="init">
		<constructor-arg value="Construct args in xml"/>
	</bean>

	<bean id="car" class="com.ustc.learn.factories.CarFactoryBean">
		<property name="carInfo" value="超级跑车,400,2000000"/>
	</bean>

	<bean name="circularBeanA" class="com.ustc.learn.beans.CircularBeanA" autowire="byType"/>

	<bean name="circularBeanB" class="com.ustc.learn.beans.CircularBeanB">
		<property name="a" ref="circularBeanA"/>
	</bean>

	<bean name="testBean" class="com.ustc.learn.beans.TestBean"/>
</beans>